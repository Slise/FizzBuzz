//  main.c
//  C-Starter-Project FizzBuzz
//
//  Created by Benson Huynh on 2015-09-29.
//

#include <stdio.h>

int main (void)
{
    int i = 1;
    
    //run number 1 to 100
    for (i = 1;i < 101;++i)
            {
                //case 1 print "Fizz" if multiple of 3
                if (i % 3 == 0)
                {
                    printf("Fizz\n");
                }
                //case 2 print "Buzz" if multiple of 5
                else if
                    (i % 5 == 0)
                {
                    printf("Buzz\n");
                }
                //case 3 print "FizzBuzz" if multiple of 3 and 5
                else if
                    (i % 3 == 0 && i % 5 == 0)
                {
                    printf("FizzBuzz\n");
                }
                //case 4 print number if not multiple of 3 or 5
                else
                {
                    printf("%d\n", i);
                }
            }
    return 0;
